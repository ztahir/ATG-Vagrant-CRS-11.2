        <xa-datasource-property name="User">crs_core</xa-datasource-property>
        <xa-datasource-property name="Password">crs_core</xa-datasource-property>
        <xa-datasource-class>oracle.jdbc.xa.client.OracleXADataSource</xa-datasource-class>
